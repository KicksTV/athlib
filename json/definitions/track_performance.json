{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Single athlete's performance in a single track event",
    "description": "A single athlete's performance in a single track event, which comprises a single attempt.",
    "type": "object",
    "properties": {
        "event": {
            "description": "Standard event type short name",
            "enum": [
                "60",
                "60H",
                "100",
                "110H",
                "200",
                "200H",
                "400",
                "400H",
                "800",
                "1500",
                "mile",
                "3000",
                "3000SC",
                "5000",
                "10000"
            ]
        },
        "performance": {
            "description": "Time taken to complete event",
            "type": [
                "number",
                "string"
            ],
            "minimum": 0,
            "minLength": 1
        }
    },
    "required": ["event", "performance"]
}
