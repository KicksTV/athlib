{
  "name": "athlib",
  "version": "0.3.7",
  "description": "Athletics Library",
  "main": "./index.js",
  "scripts": {
    "build": "npm run lint && vue-cli-service build --target lib --formats umd-min  src/athlib.js && rename dist/athlib.umd.min.js dist/athlib.web.js && rename dist/athlib.umd.min.js.map dist/athlib.web.js.map && npm run test-dist",
    "build-node": "npm run lint && vue-cli-service build --target lib --formats commonjs  --dest lib --name athlib src/athlib.js && rename lib/athlib.common.js lib/athlib.js && rename lib/athlib.common.js.map lib/athlib.js.map && npm run test-node",
    "test": "TESTSRC=\"src\" jest",
    "test-dist": "TESTSRC=\"dist\" jest",
    "test-node": "TESTSRC=\"node\" jest",
    "clean": "rimraf dist lib babelled && jest --clearCache",
    "dist-clean": "npm run clean && rimraf node_modules",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "inspect": "vue-cli-service inspect > inspect.out.js",
		"prepublishOnly": "npm run test-all",
    "test-all": "npm run test && npm run build && npm run build-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openath/athlib.git"
  },
  "keywords": [
    "athletics",
    "library"
  ],
  "author": {
    "name": "Various",
    "url": "https://github.com/openath"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openath/athlib/issues"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/openath/athlib",
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@cypress/webpack-preprocessor": "^3.0.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-e2e-cypress": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "@babel/cli": "^7.1.2",
    "rename-cli": "^5.1.2"
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
    "es-check": "^4.0.0",
    "flush-promises": "^1.0.0",
    "json-loader": "^0.5.7",
    "rimraf": "^2.6.2",
    "uiv": "^0.27.0",
    "vue-template-compiler": "^2.5.17"
  },
  "browserslist": {
    "targets": "> 0.25%, not dead"
  },
  "library": {
    "name": "athlib",
    "dist-node": "athlib.js",
    "dist-web": "athlib.web.js",
    "bundle-web": true
  },
  "dependencies": {
  }
}
