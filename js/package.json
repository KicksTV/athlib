{
  "name": "athlib",
  "version": "0.3.6",
  "description": "Athletics Library",
  "main": "./index.js",
  "scripts": {
    "build": "webpack --mode production ./src/library.js --output ./dist/athlib.web.js --module-bind js=babel-loader --output-library=\"\"",
    "unit-tests": "TESTSRC=\"src\" jest",
    "unit-tests-dist": "TESTSRC=\"dist\" jest",
    "clean": "rimraf dist lib",
    "dist-clean": "rimraf dist lib node_modules",
    "lint": "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openath/athlib.git"
  },
  "keywords": [
    "athletics",
    "library"
  ],
  "author": {
    "name": "Various",
    "url": "https://github.com/openath"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openath/athlib/issues"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/openath/athlib",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "es-check": "^4.0.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-codemods": "^0.16.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  },
  "browserslist": {
    "targets": "> 0.25%, not dead"
  },
  "jest": {
    "testRegex": "test/.*\\.(js|jsx)$"
  },
  "library": {
    "name": "athlib",
    "entry": "library.js",
    "dist-node": "athlib.js",
    "dist-web": "athlib.web.js",
    "bundle-web": true
  },
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20"
  }
}
