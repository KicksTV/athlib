{
  "name": "athlib",
  "version": "0.3.6",
  "description": "Athletics Library",
  "main": "./index.js",
  "scripts": {
    "build": "webpack --mode production ./src/library.js --output ./dist/athlib.js --module-bind js=babel-loader",
    "build-dev": "webpack --mode development ./src/library.js --output ./lib/athlib.js --module-bind js=babel-loader",
    "test-build": "npm run build && mocha --compilers js:@babel/register",
    "test": "mocha --compilers js:@babel/register",
    "clean": "rimraf dist lib",
    "dist-clean": "rimraf dist lib node_modules",
    "lint": "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openath/athlib.git"
  },
  "keywords": [
    "athletics",
    "library"
  ],
  "author": {
    "name": "Various",
    "url": "https://github.com/openath"
  },
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/openath/athlib/issues"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/openath/athlib",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  },
  "browserslist": {
    "targets": "> 0.25%, not dead"
  },
  "library": {
    "name": "athlib",
    "entry": "library.js",
    "dist-node": "athlib.js",
    "dist-web": "athlib.web.js",
    "bundle-node": true
  },
  "dependencies": {
    "es-check": "^4.0.0"
  }
}
