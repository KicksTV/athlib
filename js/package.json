{
  "name": "athlib",
  "version": "0.3.12",
  "description": "Athletics Library",
  "main": "./index.js",
  "scripts": {
    "comment": "echo I wish I could put comments in JSON.  Do we really need all these?  -Andy",
    "prepare": "npm run build-all",
    "preversion": "npm run build-all && npm run unit",
    "version": "git add .",
    "postversion": "git push && git push --tags",
    "build": "webpack --prod=1 --env=node && webpack --prod=1 --env=WEB && npm run test",
    "build-all": "npm run build",
    "test-old": "mocha --require babel-core/register",
    "lint": "eslint src/",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./index.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openath/athlib.git"
  },
  "keywords": [
    "athletics",
    "library"
  ],
  "author": {
    "name": "Various",
    "url": "https://github.com/openath"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openath/athlib/issues"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/openath/athlib",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.4",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.26.0",
    "chai": "^3.4.1",
    "clean-webpack-plugin": "^0.1.8",
    "cross-env": "^5.2.0",
    "del": "^2.2.0",
    "es-check": "^4.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.1",
    "mocha": "^6.0.0",
    "object-assign": "^4.0.1",
    "webpack": "^1.12.11",
    "webpack-node-externals": "^0.4.1",
    "yargs": "^13.2.2",
    "nyc": "^13.1.0"
  },
  "browserslist": {
    "targets": "> 0.25%, not dead"
  },
  "library": {
    "name": "athlib",
    "entry": "athlib.js",
    "dist-node": "athlib.js",
    "dist-web": "athlib.web.js",
    "bundle-node": true
  },
  "dependencies": {}
}
